<div>
    <ul>
        {% for name, param in params %}
            <li class="btn btn-default btn-xs">
                <span class="fa fa-close filter-option-remove" data-name="{{ name }}"></span>
                <strong>{{ filterLabelOptionPairs['labels'][name] }}</strong> -
                {{ filterLabelOptionPairs['options'][param] is defined ? filterLabelOptionPairs['options'][param] : param }}
            </li>
        {% endfor %}
    </ul>
</div>
