<div class="modal-header">
    <button type="button" class="close pmi-x-out hidden"><span>&times;</span></button>
    <h4 class="modal-title"><i class="fa fa-save"></i> {{ workQueueViewId is defined ? 'Edit View' : 'Save View' }}</h4>
</div>
<div class="modal-body">
    {% set queryParams = params is defined ? params : '' %}
    {% set actionUrl = workQueueViewId is defined ? path('workqueue_view', { id: workQueueViewId, params:
        queryParams, viewType: viewType, currentUrl: currentUrl }) :
        path('workqueue_view', { params: queryParams, viewType: viewType }) %}
    {{ form_start(workQueueViewForm, { attr: { action: actionUrl } }) }}
    {{ form_rest(workQueueViewForm) }}
    <button type="submit" class="btn btn-primary">Save</button>
    <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
    {{ form_end(workQueueViewForm) }}
</div>
