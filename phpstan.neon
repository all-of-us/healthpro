parameters:
    level: 3
    paths:
        - src
    bootstrapFiles:
        - vendor/autoload.php
    doctrine:
        objectManagerLoader: tests/object-manager.php
    symfony:
        container_xml_path: var/cache/dev/App_KernelDevDebugContainer.xml
    reportUnmatchedIgnoredErrors: false
    ignoreErrors:
        -
            message: '#No error to ignore is reported [a-zA-Z0-9\\_|]#'
            path: src/Helper/WorkQueue.php
